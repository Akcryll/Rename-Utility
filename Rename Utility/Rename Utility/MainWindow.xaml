<Window x:Class="Rename_Utility.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rename_Utility"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Browse Files" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="browseFiles"/>
        <Label x:Name="label" HorizontalAlignment="Left" Margin="10,47,0,0" VerticalAlignment="Top" Content="Files."/>
        <Label x:Name="lblFiles" Content="" HorizontalAlignment="Left" Margin="112,47,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.353,-0.411"/>
        <Label x:Name="label1" Content="Duplicate files" HorizontalAlignment="Left" Margin="10,86,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblDuplicateFiles" Content="" HorizontalAlignment="Left" Margin="112,86,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
